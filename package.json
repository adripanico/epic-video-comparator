{
  "name": "@epiclabs/epic-video-comparator",
  "version": "0.0.8",
  "description": "JS library to create a video comparator, i.e., two overlaped and syncrhonized video players each one with an independent source.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "_clean": "rimraf dist",
    "_transpile": "tsc",
    "_bundle": "webpack --config webpack.config.prod.js",
    "_npm_ready": "cpy CHANGELOG.md LICENSE package.json README.md ./dist/",
    "start": "webpack-dev-server --config webpack.config.dev.js --host 0.0.0.0 --port 8085 --open --mode development",
    "build": "run-s _clean _bundle _transpile _npm_ready",
    "build-dev": "webpack --config webpack.config.dev.js",
    "_lint": "tslint -c tslint.json 'src/**/*.ts'",
    "_utest": "jest --coverage --env=jsdom",
    "test": "run-s _lint _utest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epiclabs-io/epic-video-comparator.git"
  },
  "keywords": [
    "epic",
    "labs",
    "epiclabs",
    "video",
    "player",
    "dash",
    "dashjs",
    "hls",
    "hls.js"
  ],
  "author": "Adrian Caballero <https://github.com/adripanico>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/epiclabs-io/epic-video-comparator/issues"
  },
  "homepage": "https://github.com/epiclabs-io/epic-video-comparator#readme",
  "dependencies": {
    "@epiclabs/epic-video-player": "0.0.12",
    "screenfull": "5.0.0"
  },
  "devDependencies": {
    "@types/hls.js": "0.12.5",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.5",
    "css-loader": "3.2.0",
    "cpy-cli": "2.0.0",
    "dashjs": "3.0.0",
    "html-webpack-plugin": "3.2.0",
    "hls.js": "0.12.4",
    "jest": "24.9.0",
    "node-sass": "4.13.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.0",
    "tslib": "1.10.0",
    "tslint": "5.20.0",
    "typescript": "3.6.4",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.41.2",
    "webpack-auto-inject-version": "1.2.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
